[
  { "key": "alt+h", "command": "workbench.action.previousEditor" },
  { "key": "alt+l", "command": "workbench.action.nextEditor" },
  { "key": "alt+shift+h", "command": "workbench.action.moveEditorLeftInGroup" },
  { "key": "alt+shift+l", "command": "workbench.action.moveEditorRightInGroup" },
  { "key": "ctrl+h", "command": "workbench.action.navigateLeft" },
  { "key": "ctrl+l", "command": "workbench.action.navigateRight" },
  { "key": "ctrl+j", "command": "workbench.action.navigateDown" },
  { "key": "ctrl+k", "command": "workbench.action.navigateUp" },

  // suggestions
  { "key": "ctrl+j", "command": "selectNextSuggestion", "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible" },
  { "key": "ctrl+k", "command": "selectPrevSuggestion", "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible" },

  // quick open
  { "key": "ctrl+j", "command": "workbench.action.quickOpenNavigateNextInViewPicker", "when": "inQuickOpen" },
  { "key": "ctrl+k", "command": "workbench.action.quickOpenNavigatePreviousInViewPicker", "when": "inQuickOpen" },

  { "key": "ctrl+oem_3", "command": "workbench.action.togglePanel"},
  { "key": "ctrl+1", "command": "terminal.focus" },
  { "key": "ctrl+2", "command": "workbench.panel.markers.view.focus" },
  { "key": "ctrl+3", "command": "workbench.panel.output.focus" },
  { "key": "ctrl+4", "command": "workbench.debug.action.focusRepl" },

  { "key": "ctrl+oem_6", "command": "workbench.action.moveEditorToRightGroup" },
  { "key": "ctrl+oem_4", "command": "workbench.action.moveEditorToLeftGroup" },

  { "key": "f5", "command": "workbench.action.debug.selectandstart" },
  { "key": "ctrl+shift+,", "command": "workbench.action.openGlobalKeybindingsFile" },
  { "key": "ctrl+shift+b", "command": "workbench.action.toggleAuxiliaryBar" },

  { "key": "shift+k", "command": "editor.action.showHover", "when": "editorTextFocus" },
]
